<?php

/**
 * Implements hook_init().
 */
function nodejs4d6_notify_init() {
  //  drupal_add_library('system', 'ui.dialog');
  drupal_add_css(drupal_get_path('module', 'nodejs4d6_notify') . '/libraries/jgrowl/jquery.jgrowl.css');
  drupal_add_js(drupal_get_path('module', 'nodejs4d6_notify') . '/libraries/jgrowl/jquery.jgrowl.js');
}

/**
 * Implements hook_nodejs4d6_handlers_info().
 */
function nodejs4d6_notify_nodejs4d6_handlers_info() {
  return array(
    drupal_get_path('module', 'nodejs4d6_notify') . '/nodejs4d6.notify.js',
  );
}
